<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.MateusVere">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.N2TSI"
        tools:targetApi="31">
        <activity
            android:name="com.example.MateusVere.UsuarioFavoritos"
            android:windowSoftInputMode="adjustPan"
            android:exported="false" />
        <activity
            android:name="com.example.MateusVere.UsuarioLogado"
            android:windowSoftInputMode="adjustPan"
            android:exported="false" />
        <activity
            android:name="com.example.MateusVere.CriaUsuario"
            android:exported="false" />
        <activity
            android:name="com.example.MateusVere.RecuperaSenha"
            android:exported="false" />
        <activity
            android:name="com.example.MateusVere.MainActivity"
            android:windowSoftInputMode="adjustPan"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>